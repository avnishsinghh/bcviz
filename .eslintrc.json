{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [ "error", 2, {
      "ignoredNodes": ["ConditionalExpression"],
      "FunctionDeclaration": {"parameters": "first"},
      "CallExpression": {"arguments": "first"}
    }],
    "max-len": [ "error", 120 ]

  }
}
